{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "schema for wechat signature api",
  "type": "object",
  "properties": {
    "url": {
      "type": "string",
      "pattern": "^http"
    },
    "type": {
      "type": "string",
      "enum": ["hom", "changxu", "maqian", "liuting", "shanshan", "zhanglin", "hulei", "congcong", "kiwi", "nonobank", "mxd", "unifi", "mzqbj"]
    }
  },
  "required": ["type", "url"]
}